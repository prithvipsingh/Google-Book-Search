{
  "name": "google-search-book",
  "version": "1.0.0",
  "description": "google search book app",
  "main": "server.js",
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "axios": "^0.19.0",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.6.7"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently -k \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client && npm install --only=dev && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prithvipsingh/Google-Book-Search.git"
  },
  "author": "Prithvi Pal Singh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prithvipsingh/Google-Book-Search/issues"
  },
  "homepage": "https://github.com/prithvipsingh/Google-Book-Search#readme"
}
